{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"types": ["bun"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"noImplicitAny": true,
		"paths": {
			"~/*": ["./src/*"]
		},
		"baseUrl": ".",
		"skipLibCheck": true,
		"experimentalDecorators": true
	}
}
